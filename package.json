{
  "name": "react-better-example",
  "version": "0.1.10",
  "description": "Backend separated & optimized bundling & i18n support",
  "main": "dist/app.js",
  "scripts": {
    "build": "node_modules/.bin/gulp build",
    "clean": "rm -rf dist",
    "lint": "echo 'js\\njsx' | xargs -I {} find js -name *.{} | xargs node_modules/.bin/jsxhint -c ~/.jshintrc",
    "publish": "node_modules/.bin/gulp publish && npm run uglify",
    "uglify": "ls dist/*.js | xargs -I {} node_modules/.bin/uglify -s {} -o {}",
    "watch": "node_modules/.bin/gulp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wizawu/react-better-example.git"
  },
  "keywords": [
    "react",
    "example",
    "template"
  ],
  "author": "wizawu",
  "license": "MIT",
  "devDependencies": {
    "babelify": "*",
    "browserify": "*",
    "global-react": "*",
    "gulp": "*",
    "gulp-concat": "*",
    "gulp-template": "*",
    "jsxhint": "^0.12.0",
    "uglify": "*",
    "vinyl-source-stream": "*",
    "watchify": "*"
  },
  "dependencies": {
    "events": "^1.0.2",
    "flux": "^2.0.3",
    "i18next-client": "^1.10.2",
    "mockxhr": "*",
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  }
}
